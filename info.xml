<?xml version="1.0"?>
<extension key="action-provider-addon" type="module">
    <file>action_provider_addon</file>
    <name>Action Provider Addons</name>
    <description>Extra actions for action-provider</description>
    <license>AGPL-3.0</license>
    <maintainer>
        <author>Sandor Semsey</author>
        <email>sandor@es-progress.hu</email>
    </maintainer>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/action-provider-addon</url>
        <url desc="Documentation">https://github.com/reflexive-communications/action-provider-addon</url>
        <url desc="Support">https://github.com/reflexive-communications/action-provider-addon/issues</url>
        <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2022-04-05</releaseDate>
    <version>1.0.1</version>
    <develStage>alpha</develStage>
    <compatibility>
        <ver>5.37</ver>
    </compatibility>
    <requires>
        <ext>action-provider</ext>
    </requires>
    <classloader>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <civix>
        <namespace>CRM/ActionProviderAddon</namespace>
    </civix>
</extension>
